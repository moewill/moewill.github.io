            © 2024 Maurice Rashad. All rights reserved.
        </div>
    </footer>

    <!-- JavaScript -->
    <script src="js/main.js"></script>
    <script src="js/chatbot-config.js?v=2.0"></script>
    <script src="js/claude-chatbot.js?v=2.0"></script>
    <script src="js/wow-effects.js?v=2.0"></script>
    <script src="js/easter-eggs.js?v=1.0"></script>
    <script src="js/interactive-elements.js?v=1.0"></script>
    <script src="js/apple-effects.js?v=1.0"></script>
    
    <script>
        // Apple-style enhancements
        document.addEventListener('DOMContentLoaded', () => {
            // Form submission with Apple-style feedback
            const appleForm = document.getElementById('apple-contact-form');
            if (appleForm) {
                appleForm.addEventListener('submit', (e) => {
                    e.preventDefault();
                    
                    const submitBtn = appleForm.querySelector('button[type="submit"]');
                    const originalText = submitBtn.innerHTML;
                    
                    // Apple-style loading state
                    const resetLoading = appleEffects.showLoadingState(submitBtn, 'Sending...');
                    
                    // Simulate submission
                    setTimeout(() => {
                        resetLoading();
                        submitBtn.innerHTML = '<i class="fas fa-check"></i> Message Sent!';
                        submitBtn.style.background = 'linear-gradient(135deg, #10B981, #059669)';
                        
                        // Show success notification
                        if (window.wowEffects) {
                            wowEffects.showNotification('✨ Message sent successfully! We\'ll be in touch within 24 hours.', 'success', 6000);
                        }
                        
                        // Reset form after delay
                        setTimeout(() => {
                            appleForm.reset();
                            submitBtn.innerHTML = originalText;
                            submitBtn.style.background = '';
                        }, 3000);
                    }, 2000);
                });
            }
        });
    </script>
</body>
</html>